.main {
  padding-bottom: 140rpx;
}

.title_item {
  height: 100rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
  padding: 0 32rpx;
  background: #f8f8f8;
}

.title_item image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 16rpx;
}

.c_text {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #01C48F;
}

.c_text1 {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #01C48F;
}

.c_text2 {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #FF6600;
}


.child_info {
  background: #ffffff;
}

.child_mem_info {
  padding: 0 32rpx 20rpx;
  background: #ffffff;
}

.c_detail_info {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
}

.c_detail_info view {
  flex: 1;
}

.c_name {
  font-size: 40rpx;
  font-family: Source Han Sans CN;
  font-weight: bold;
  color: #01C48F;
}

.c_no {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
}

.c_detail_info1 {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 80rpx;
}

.c_detail_info1>view {
  flex: 1;
}

.c_detail {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
}

.c_info1 {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
  background: #ffffff;
}

.info2_info1 {
  border-bottom: 1px solid #eeeeee;
  padding: 0 32rpx 32rpx;
}

.info_item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #eeeeee;
  padding: 0 32rpx;
}

.c_info1>view text {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
  opacity: 0.5;
  margin-left: 10rpx;
}

.info_text {
  height: 100rpx;
  line-height: 100rpx;
}

.info_desc {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  line-height: 32rpx;
  color: #CCCCCC;
}

.eye_info {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
  padding: 0 32rpx;
}

.eye_info>view {
  flex: 0.5;
}

.eye_info>view:nth-child(1) {
  flex: 0.5;
}

.eye_info>view:nth-child(2) {
  margin-right: 180rpx;
}

.eye_info>view:nth-child(2),
.eye_info>view:nth-child(3) {
  text-align: center;
}

.eye_item {
  padding: 0;
  margin: 0 32rpx;
}

.no_eye_view {
  padding: 32rpx;
  border-bottom: 1px solid #EEEEEE;
}

.no_eye_info {
  display: flex;
  align-items: center;
}

.no_eye_info>view {
  flex: 1;
  text-align: center;
  line-height: 80rpx;
}

.c_text2 image {
  width: 20rpx;
  height: 33rpx;
  margin-right: 40rpx;
}

.sup {
  display: inline-block;
  vertical-align: super;
  margin-left: 0 !important;
  font-size: 16rpx !important;
}

.info_mem {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.info_mem>view:nth-child(1) {
  flex: 1;
}

.canvas_view {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 180rpx;
}

.tooth_desc {
  width: 50%;
  display: flex;
  flex-wrap: wrap;
}

.tooth_desc>view {
  width: 49%;
  text-align: center;
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
}

.tooth_desc>view:nth-child(1) {
  border-right: 1px solid #000000;
  border-bottom: 1px solid #000000;
}

.tooth_desc>view:nth-child(2) {
  border-bottom: 1px solid #000000;
}

.tooth_desc>view:nth-child(3) {
  border-right: 1px solid #000000;
}

.top_b {
  border-top: 1px solid #eeeeee;
}

.tips_text {
  font-size: 28rpx;
  padding: 32rpx;
  color: rgba(0, 0, 0, 0.8);
  background: #ffffff;
  border-bottom: 1px solid #eeeeee;
}


.jcjl_tip {
  font-size: 28rpx;
  color: #000000;
  font-weight: bold;
  padding-bottom: 16rpx;
}

.cljy_tip {
  font-size: 28rpx;
  color: #22bdbc;
  font-weight: bold;
  padding-bottom: 16rpx;
}

.end_text {
  background: transparent;
  border: none;
  font-size: 28rpx;
  color: #000000;
}

.tips_text text {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #333333;
}

.tips_text label {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: red;
}

.app-btn {
  position: fixed;
  bottom: 40rpx;
  width: 430rpx;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  background: #01C48F;
  box-shadow: 0px 6rpx 12rpx rgba(0, 0, 0, 0.16);
  border-radius: 72px;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 1;
  font-size: 28rpx;
  font-family: Avenir Next LT Pro;
  font-weight: 400;
  color: #FFFFFF;
}

.eye_new_info>view {
  flex: 0.5;
}

.eye_new_info>view:nth-child(1) {
  flex: 0.5;
}

.eye_new_info>view:nth-child(2) {
  margin-right: 0;
}

.eye_new_info>view:nth-child(2),
.eye_new_info>view:nth-child(3),
.eye_new_info>view:nth-child(4) {
  text-align: center;
}

.remark-view {
  border-top: 1px solid #EEEEEE;
  border-bottom: none;
}

.eye_remark {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
  margin-bottom: 12rpx;
}

.no_b {
  border-bottom: none;
}

.in-b-b {
  border-top: 4rpx solid #EEEEEE;
}

.in-b-b {
  border-top: 4rpx solid #EEEEEE;
}

.eye-ii {
  padding: 10rpx 80rpx;
}

.no_e_info {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.no_e_info>view:first-child {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
  width: 60%;
}

.no_e_info>view:last-child {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #01C48F;
}

.n_e_i>view:nth-child(2) {
  margin-right: 0;
}

.n_e_i>view:nth-child(1) {
  flex: 0.8;
}

.img-view {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 40rpx 90rpx;
  border-bottom: 1px solid #eeeeee;
}

.img-view>view {
  width: 210rpx;
  height: 210rpx;
  background: #EEEEEE;
}

.img-view>view>image {
  width: 100%;
  height: 100%;
}

.e_r_view {
  padding: 30rpx 0;
}

.e_r_view>view {
  display: flex;
  align-items: center;
  justify-content: space-between;
  text-align: center;
}

.e_r_view>view>view {
  flex: 1;
  line-height: 60rpx;
}