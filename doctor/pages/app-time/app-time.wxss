.main {
  padding-bottom: 40rpx;
}

.doc-name {
  font-size: 36rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #000000;
  text-align: center;
  padding: 20rpx 0 0;
}

.time-view {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 28rpx 12rpx;
  position: sticky;
  top: 0;
  background: #F8F8F8;
  z-index: 1;
  box-shadow: 0 8rpx 8rpx 0 rgba(0, 0, 0, 0.05);
}

.time-item {
  width: 120rpx;
  height: 140rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 6rpx solid transparent;
}

.time-dd>view:first-child {
  font-size: 24rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #000000;
}

.time-dd>view:last-child {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.5);
  margin-top: 18rpx;
}

.curr {
  border: 6rpx solid #01C48F;
}

.curr-text {
  color: #01C48F !important;
}

.app-view {
  padding: 0 20rpx;
}

.app-item {
  width: 100%;
  height: 112rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin-top: 16rpx;
}

.app-time-text {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.5);
  margin-left: 16rpx;
}

.app-left {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #000000;
}

.app-left label {
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #01C48F;
}

.over-text {
  color: #CCCCCC !important;
}

.over-app {
  width: 124rpx;
  height: 48rpx;
  line-height: 48rpx;
  background: transparent;
  border-radius: 8rpx;
  font-size: 24rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.5);
  text-align: center;
  margin-right: 16rpx;
}

.in-app {
  width: 124rpx;
  height: 48rpx;
  line-height: 48rpx;
  background: #FF6600;
  border-radius: 8rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #FFFFFF;
  text-align: center;
  margin-right: 16rpx;
}